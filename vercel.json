{
    "builds": [
        {
            "src": "/server/api.py",
            "use": "@now/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server/api.py"
        }
    ]
}
